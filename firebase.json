{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/login",
        "destination": "/login.html"
      },  
      {
        "source": "/login",
        "destination": "/login.html"
      }
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },

      {
        "source":"/main",
        "destination": "/main.html"
      }
    ]
  }
}
